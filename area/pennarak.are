      if( find_stat( ch, dex ) > random( 5, 20 ) ) {
        send_to_char( #Save, ch );
        transfer( ch, find_room( 36318 ) );
        i = 1d6;
        dam_message( ch, i, "sliding down the cliff" );
        inflict( ch, mob, i, "sliding down the cliff" );
        }
      else {
        i = 9d6;
        acode( find_room( 36254 ), 2 );
        }
      }
