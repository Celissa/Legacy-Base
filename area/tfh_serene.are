if( find_stat( ch, level ) < 98 ) {
  if( rflag( reset1, room ) ) {
    act_tochar( #move, ch );
    i = random( 1, 9 );
    if( i == 1 )
      j = 11049;
    if( i == 2 )
      j = 11044;
    if( i == 3 )
      j = 11045;
    if( i == 4 )
      j = 11046;
    if( i == 5 )
      j = 11047;
    if( i == 6 )
      j = 11028;
    if( i == 7 )
      j = 11027;
    if( i == 8 )
      j = 11026;
    if( i == 9 )
      j = 11048;
    transfer( ch, find_room( j ) );
    if( is_player( ch ) )
      act_notchar( #arrive, ch );
    if( !is_player( ch ) )
      act_notchar( #mobarrive, ch );
    loop( followers ) {
      if( !is_sleeping( rch ) ) {
        if( !is_resting( rch ) ) {
          act_notchar( #move2, ch );
          transfer( rch, find_room( j ) );
          act_notchar( #arrive, rch );
          }
        }
      }
    }
  else
    continue;
  }
else
  continue;
