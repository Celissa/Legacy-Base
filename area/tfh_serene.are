if( find_stat( ch, level ) < 98 ) {
  if( rflag( reset1, room ) ) {
    act_tochar( #move, ch );
    act_notchar( #gone, ch );
    i = random( 1, 3 );
    if( i == 1 )
      j = 11035;
    if( i == 2 )
      j = 11042;
    if( i == 3 )
      j = 11043;
    transfer( ch, find_room( j ) );
    if( is_player( ch ) )
      act_notchar( #arrive, ch );
    if( !is_player( ch ) )
      act_notchar( #mobarrive, ch );
    loop( followers ) {
      if( !is_sleeping( rch ) ) {
        if( !is_resting( rch ) ) {
          act_notchar( #move2, ch );
          transfer( rch, find_room( j ) );
          act_notchar( #arrive, rch );
          }
        }
      }
    }
  else
    continue;
  }
else
  continue;
